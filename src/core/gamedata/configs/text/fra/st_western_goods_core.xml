    </string>

    <string id="st_wg_missing_requirements_ext">
        <text>Un outil est nécessaire!</text>
    </string>

    <string id="st_wg_not_enough_fuel">
        <text>Pas assez de carburant</text>
    </string>

    <string id="st_wg_lighter_header">
        <text>%s •%s Carburant: %s%s%\n \n%s%s</text>
    </string>

    <!-- Suitcase use strings -->
    <string id="st_wg_open">
        <text>Ouvrir</text>
    </string>

    <!-- Readable UI strings -->
    <string id="ui_wg_dx8_retard_notice">
        <text>Il semble que vous utilisez le rendu DirectX 8, ce qui peut entraîner des étirements de textures. Utilisez un rendu plus moderne pour éviter ces problèmes.</text>
    </string>

    <!-- Map spots strings -->
    <string id="st_wg_map_spot_helicopter">
        <text>Hélicoptère</text>
    </string>

    <string id="st_wg_map_spot_enemy">
        <text>Ennemi</text>
    </string>

    <string id="st_wg_map_spot_gps_stash">
        <text>Cache marquée par GPS</text>
    </string>

    <!-- Misc strings -->
